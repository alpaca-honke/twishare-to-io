{
	"manifest_version": 3,
	"name":"Twishare to io",
	"version":"0.0.0",
	"description":"Twitter のシェアボタンから Misskey.io シェアすることができるようにします。",
	"icons":{
		"16":"16.png", 
		"48":"48.png",
		"128":"128.png"
	},
	"content_scripts":[
		{
			"run_at":"document_start",
			"matches":["https://twitter.com/intent/tweet*","http://twitter.com/intent/tweet*"],
			"js":["script.js"]
		}
	],
	"author":"Alkappa/alpaca-honke",
	"homepage_url":"https://alpaca-honke.github.io/"
}